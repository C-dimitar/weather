<body>

	<div id="splash_screen">

		<div class="logo">
			<img src="/img.gif">
		</div>

		<h1>Wheather</h1>

		<!--<h3>© Copyright, Skopje 2017</h3>-->

	</div>

	<div class="container">

		<div class="col-md-12">
			<nav class="navbar navbar-default" role="navigation">
				<div class="navbar-header">

					<button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#navbarNav">
						<span class="sr-only">Toggle navigation</span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
						<span class="icon-bar"></span>
					</button>
					<a class="navbar-brand" href="index.html">
						<img id="logo" src="/weather.jpg" />
					</a>
				</div>

				<!-- Input -->
				<div class="collapse navbar-collapse" id="#navbarNav">
					<form class="navbar-form navbar-left" action="/" method="POST">
						<div class="form-group">
							<input type="text" id="search-input" name="location" placeholder="Search for a city" class="form-control" />
						</div>
					</form>
					<button type="button" id="btn-locate" class="btn pull-right btn-locate">Најди ме</button>
				</div>

			</nav>
		</div>

		<div class="row">
			<div class="col-md-12">
				<div class="row">
					<div class="col-md-6">
						<span id="cityName">CityName</span>

					</div>
				</div>

				<h3 id="state">State Name</h3>
				<br />
				<h4 id="dateTime"></h4>

				<div class="left-bottom">
					<div id="weather-description">
						<img src="#" />
						<span>Description</span>
					</div>
					<div class="min-max">
						<span class="max">Max temp</span>
						<span> &#176 C</span>
						<span class="min">Mintemp</span>
						<span> &#176 C</span>
					</div>
					<div class="current-temp">
						<span id="temp-value">2</span>
						<span> &#176C</span>
					</div>
				</div>

			</div>

		</div>
		<div class="row">
			<div class="col-md-6">
				<div class="panel panel-default">
					<div class="panel-heading">
						<h3 class="panel-title">
							Детали
						</h3>
					</div>
					<div class="panel-body">
						<ul id="ul-details" style="background-color:none;">
							<li id="could-coverage">
								<span class="details-label">Облачност</span>
								<span class="details-unit"> %</span>
								<span class="details-info"></span>
							</li>

							<li id="pressure">
								<span class="details-label">Притисок</span>
								<span class="details-unit"> hPa</span>
								<span class="details-info"></span>
							</li>

							<li id="visibility">
								<span class="details-label">Видливост</span>
								<span class="details-unit"> km</span>
								<span class="details-info"></span>
							</li>

							<li id="humidity">
								<span class="details-label">Влажност</span>
								<span class="details-unit"> %</span>
								<span class="details-info"></span>
							</li>

							<li id="sunrise">
								<span class="details-label">Изгрејсонце</span>
								<span class="details-info"></span>
							</li>

							<li id="sunset">
								<span class="details-label">Зајдисонце</span>
								<span class="details-info"></span>
							</li>

							<li id="wind">
								<span>Ветер</span>
								<ul>
									<li id="degrees">
										<span class="details-label">Степени</span>
										<span class="details-unit"> &#176</span>
										<span class="details-info"></span>
									</li>
									<li id="speed">
										<span class="details-label">Брзина </span>
										<span class="details-unit"> km/h</span>
										<span class="details-info"></span>
									</li>
								</ul>
							</li>

						</ul>
					</div>
				</div>

			</div>
			<div class="col-md-6">
				<div id="map">

				</div>
			</div>
		</div>
	</div>

